<h1>Add new role</h1>
<div>
<%= form_for @role do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
    <div><%= f.label :name %>
    <%= f.text_field :name %></div>

    <%= f.submit "Submit", class: "btn btn-large"%>
<% end %>
<%= link_to "Back", :back %>

<% for role in Role.all %>
	<%= role.name.underscore %>
<% end %>
</div>